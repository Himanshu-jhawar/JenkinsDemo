pipeline{
    agent any
    
    stages{
        stage('Starting'){
            steps{
                echo'Started the build'
            }        
        }
        
        stage('Building'){
            steps{
                echo'Building the Project'
            }        
        }
        
        stage('Deploying'){
            steps{
                echo'Deploying the Project'
            }        
        }
        
        stage('Done'){
            steps{
                echo'Build Successfully Done'
            }        
        }
    }     
}
